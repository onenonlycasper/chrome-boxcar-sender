{ 
  "name": "Boxcar Sender", 
  "version": "0.1.10", 
  "description": "Quick send text from Chrome to iOS via Boxcar.", 
  "manifest_version": 2,
  "background" : {
    "page": "background.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://boxcar.io/*",
    "storage",
    "notifications"
  ],
  "icons": {
    "16" : "16.png",
    "48" : "48.png",
    "128" : "128.png"
  },
  "options_page": "options.html",
  "browser_action": { 
    "default_icon": "19.png" ,
    "default_title": "Boxcar Sender",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "48.png"
  ],
  "minimum_chrome_version": "20",
  "content_scripts": [ {
    "js": [ "./js/boxcar.js" ],
    "matches": [ "<all_urls>" ],
    "all_frames": true
  }]
}